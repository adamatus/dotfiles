[user]
	name = Adam Riggall
	email = ariggall@gmail.com
[merge]
	tool = vimdiff
[alias]
	co = checkout
    ci = commit
	br = branch
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
    type = cat-file -t
    dump = cat-file -p
[color]
	ui = true
